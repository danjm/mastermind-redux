{
  "name": "js-full-stack",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Dan Miller <danjm.com@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gulp"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-icons": "^2.2.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "del": "^2.2.2",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "webpack-stream": "^3.2.0"
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "plugins": [
      "import"
    ],
    "rules": {
      "indent": [
        "error",
        "tab",
        {}
      ],
      "no-tabs": 0,
      "comma-dangle": 0,
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "react/jsx-indent": [
        2,
        "tab"
      ],
      "react/jsx-indent-props": [
        1,
        "tab"
      ]
    }
  }
}
